{
    "providers": {
        "tiktok.com": {
            "urlPattern": "^https?:\\/\\/(?:[a-z0-9-]+\\.)*?tiktok\\.com",
            "rules": ["is_copy_url", "is_from_webapp" ,"sender_device",
                      "sender_web_id", "mid", "sec_user_id", "share_item_id",
                      "share_link_id", "tt_from"]
        },
        "nicovideo.jp": {
            "urlPattern": "^https?:\\/\\/(?:[a-z0-9-]+\\.)*?nicovideo\\.jp",
            "rules": ["cmnhd_ref", "track", "ref"]
        },
        "cna.com.tw": {
            "urlPattern": "^https?:\\/\\/(?:[a-z0-9-]+\\.)*?cna\\.com\\.tw",
            "rules": ["utm_source", "utm_medium", "utm_campaign"]
        },
        "bilibili.com": {
            "urlPattern": "^https?:\\/\\/(?:[a-z0-9-]+\\.)*?bilibili\\.com",
            "rules": ["share_medium", "share_plat", "share_source", "share_tag",
                      "timestamp", "unique_k", "bbid", "ts"]
        },
        "myaccount.google.com": {
            "urlPattern": "^https?:\\/\\/myaccount\\.google\\.com",
            "rules": ["utm_source", "utm_medium"]
        },
        "yahoo.co.jp": {
            "urlPattern": "^https?:\\/\\/(?:[a-z0-9-]+\\.)*?yahoo\\.co\\.jp",
            "rules": ["sc_e", "utm_source", "utm_medium", "utm_campaign"]
        },
        "baidu.com": {
            "urlPattern": "^https?:\\/\\/(www\\.)?baidu\\.com\\/s(\\?[^\\?]*)?$",
            "rules": ["rsv_\\w+"]
        }
    }
}
